{
  "name": "cypressautomation",
  "version": "1.0.0",
  "description": "\"Learning Automation\"",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cy:update-snapshots" : "cypress run --env updateSnapshot=true",
    "cy:run-test-with-reporter" : "cypress run --reporter cypress-image-snapshot/reporter",
    "cy:run-test-with-no-failure" : "cypress run --env failOnSnapshotDiff=false", 
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Cypress",
    "Automation"
  ],
  "author": "Khushal Gupta",
  "license": "ISC",
  "dependencies": {
    "cypress": "^5.4.0",
    "cypress-image-snapshot": "^3.1.1",
    "cypress-xpath": "^1.6.0",
    "prettier": "^2.1.2"
  }
}
